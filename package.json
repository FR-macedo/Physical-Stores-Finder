{
  "name": "physical-stores-api",
  "version": "1.0.0",
  "description": "API para localização de lojas físicas próximas ao CEP do usuário",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "seed": "ts-node mockData.ts",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "typescript",
    "express",
    "api",
    "geolocation",
    "viacep"
  ],
  "author": "FR-macedo",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "mongoose": "^8.0.1",
    "physical-stores-api": "file:",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.9.4",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
